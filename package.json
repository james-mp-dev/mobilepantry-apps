{
  "name": "mobilepantry-apps-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for MobilePantry applications",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json,mjs,cjs}\" --ignore-path ./.prettierignore",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.11.1"
}
